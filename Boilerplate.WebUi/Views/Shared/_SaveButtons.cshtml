@model SaveButtonsViewModel
@{
    Layout = "~/Views/Shared/_LayoutBasic.cshtml";
}
<div class="linhaSolida"></div>
<div class="row">
    <div class="col-12 mb-3" style="text-align: right" >
        <button type="submit" id="button-submit" value="create" class="btn btn btn-outline-success">
                <span class="tf-icons bx bx-save"></span>&nbsp; @Model.BotaoSalvarTexto
        </button>
        <a  asp-action="index" asp-controller="@Model.Controller" class="btn btn-outline-secondary action-loading">
            <span class="tf-icons bx bx-arrow-back"></span>&nbsp; @Model.BotaoVoltarTexto
        </a>
    </div>
</div>
